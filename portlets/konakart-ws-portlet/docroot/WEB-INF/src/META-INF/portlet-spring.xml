		<property name="service" ref="com.liferay.konakart.service.LProductLocalService" />
	</bean>
	<bean id="com.liferay.konakart.service.LReviewLocalService" class="com.liferay.konakart.service.impl.LReviewLocalServiceImpl" />
	<bean id="com.liferay.konakart.service.LReviewLocalServiceUtil" class="com.liferay.konakart.service.LReviewLocalServiceUtil">
		<property name="service" ref="com.liferay.konakart.service.LReviewLocalService" />
	</bean>
</beans>